<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.1.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_1_0.dtd">

<routes>
	<route>
		<pattern>/{structId}/{categoryId}/{articleId}/{version}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_categoryId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_structId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_articleId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_version"/>
	</route>
	 
	<route>
		<pattern>/{structId}/{articleId}/{version}</pattern>
		<ignored-parameter name="p_p_id" />
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_structId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_articleId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_version"/>
	</route>
	 
	<route>
		<pattern>/{articleId}/{version}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_articleId"/>
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_version"/>
	</route>
	
	<route>
		<pattern>/{urlTitle}</pattern>
		<ignored-parameter name="p_p_cacheability" />
		<ignored-parameter name="_newsdetail_WAR_bacninhportlet_urlTitle"/>
	</route>
	
</routes>
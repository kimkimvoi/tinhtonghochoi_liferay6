<root>
	<dynamic-element name="description" type="text_box" index-type="keyword" repeatable="false">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="required"><![CDATA[true]]></entry>
			<entry name="instructions"><![CDATA[]]></entry>

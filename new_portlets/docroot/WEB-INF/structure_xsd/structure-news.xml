	<dynamic-element name="type" type="list" index-type="" repeatable="false">
		<meta-data>
			<entry name="displayAsTooltip"><![CDATA[true]]></entry>
			<entry name="instructions"><![CDATA[Select tpye article]]></entry>
			<entry name="label"><![CDATA[Type]]></entry>
			<entry name="predefinedValue"><![CDATA[]]></entry>
		</meta-data>
		<dynamic-element name="local-news" type="local-news" index-type="" repeatable="false"></dynamic-element>

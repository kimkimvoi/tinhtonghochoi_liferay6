<?xml version="1.0"?>
<!DOCTYPE layout-templates PUBLIC "-//Liferay//DTD Layout Templates 6.1.0//EN" "http://www.liferay.com/dtd/liferay-layout-templates_6_1_0.dtd">

<layout-templates>
	<custom>
		<layout-template id="three-column" name="Three Column">
			<template-path>/three-column.tpl</template-path>
			<wap-template-path>/three-column.wap.tpl</wap-template-path>
			<thumbnail-path>/three-column.png</thumbnail-path>
		</layout-template>
		<layout-template id="two-column" name="Two Column">
			<template-path>/two-column.tpl</template-path>
			<wap-template-path>/two-column.wap.tpl</wap-template-path>
			<thumbnail-path>/two-column.png</thumbnail-path>
		</layout-template>
		<layout-template id="home-page-layout"
			name="Home Page Layout">
			<template-path>/home-page-layout.tpl</template-path>
			<wap-template-path>
				/home-page-layout.wap.tpl
			</wap-template-path>
			<thumbnail-path>/home-page-layout.png</thumbnail-path>
		</layout-template>
		<layout-template id="visitorpagelayouttemplate"
			name="Visitor Page Layout">
			<template-path>
				/visitor-page-layout.tpl
			</template-path>
			<wap-template-path>
				/visitor-page-layout.wap.tpl
			</wap-template-path>
			<thumbnail-path>
				/visitor-page-layout.png
			</thumbnail-path>
		</layout-template>
	</custom>
</layout-templates>